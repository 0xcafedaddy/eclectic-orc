writeColumnSetter(schemaType) ::= <<
    <if(schemaType.typeBinary)>
        <columnBinary(schemaType)>
    <elseif(schemaType.typeChar)>
        <columnChar(schemaType)>
    <elseif(schemaType.typeVarchar)>
        <columnVarchar(schemaType)>
    <elseif(schemaType.typeString)>
        <columnString(schemaType)>
    <elseif(schemaType.typeBoolean)>
        <columnBoolean(schemaType)>
    <elseif(schemaType.typeByte)>
        <columnByte(schemaType)>
    <elseif(schemaType.typeShort)>
        <columnShort(schemaType)>
    <elseif(schemaType.typeInt)>
        <columnInt(schemaType)>
    <elseif(schemaType.typeLong)>
        <columnLong(schemaType)>
    <elseif(schemaType.typeDate)>
        <columnDate(schemaType)>
    <elseif(schemaType.typeDecimal)>
        <columnDecimal(schemaType)>
    <elseif(schemaType.typeFloat)>
        <columnFloat(schemaType)>
    <elseif(schemaType.typeDouble)>
        <columnDouble(schemaType)>
    <elseif(schemaType.typeList)>
        <columnList(schemaType)>
    <elseif(schemaType.typeTimestamp)>
        <columnTimestamp(schemaType)>
    <endif>
>>